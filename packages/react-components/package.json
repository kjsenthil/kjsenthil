{
  "name": "@tsw/react-components",
  "version": "1.0.0",
  "main": "src/index.ts",
  "description": "React components shared across all TSW websites",
  "publishConfig": {
    "registry": "https://github.com/TSWDTS/digital-hybrid.git"
  },
  "engines": {
    "node": "^14.16.0",
    "yarn": "^2.4.0"
  },
  "scripts": {
    "build-storybook": "build-storybook -s ./static",
    "compile": "tsc",
    "format": "yarn g:format --write src/**/*.{js,ts,tsx,md,json}; yarn lint:fix",
    "lint": "mkdir -p eslintreport && yarn lint:check --format html > eslintreport/index.html",
    "lint:check": "yarn g:lint src/**/*.{js,ts,tsx}",
    "lint:fix": "yarn lint:check --fix",
    "storybook": "start-storybook -p 6006 -s ./static",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverage && yarn jest-coverage-badges output './badges'",
    "typecheck": "yarn g:typecheck",
    "typecheck:watch": "yarn typecheck -w --pretty"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.4",
    "@react-spring/web": "^9.2.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@visx/axis": "^1.7.0",
    "@visx/curve": "^1.7.0",
    "@visx/event": "^1.7.0",
    "@visx/gradient": "^1.7.0",
    "@visx/grid": "^1.7.0",
    "@visx/group": "^1.7.0",
    "@visx/responsive": "^1.7.0",
    "@visx/scale": "^1.7.0",
    "@visx/shape": "^1.7.0",
    "@visx/text": "^1.7.0",
    "@visx/tooltip": "^1.7.2",
    "csstype": "^3.0.8",
    "d3-array": "^2.12.1",
    "d3-format": "^2.0.0",
    "d3-scale": "^3.3.0",
    "d3-shape": "^2.1.0",
    "d3-time-format": "^3.0.0",
    "dayjs": "^1.10.4",
    "gatsby": "^2.26.1",
    "react": "^16.12.0",
    "react-disable": "^0.1.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^13.1.5",
    "@tsw/eslint-config": "workspace:packages/shared-config/eslint",
    "@tsw/test-util": "workspace:packages/utils/test",
    "@tsw/typescript-config": "workspace:packages/shared-config/typescript",
    "@types/d3-array": "^2.9.0",
    "@types/d3-format": "^2.0.0",
    "@types/d3-interpolate-path": "^2.0.0",
    "@types/d3-time-format": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-gatsby": "^0.11.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "jest-extended": "^0.11.5",
    "jest-html-reporter": "^3.3.0",
    "jest-styled-components": "^7.0.5",
    "mockdate": "^3.0.5"
  }
}

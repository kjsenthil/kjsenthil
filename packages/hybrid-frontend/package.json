{
  "name": "@tsw/hybrid-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Front end code for the new Digital Hybrid project (Bestinvest Beta)",
  "publishConfig": {
    "registry": "https://github.com/TSWDTS/digital-hybrid.git"
  },
  "engines": {
    "node": "^14.16.0",
    "yarn": "^2.4.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "yarn g:format --write src/**/*.{js,ts,tsx,md,json}; yarn lint:fix",
    "format:all": "yarn g:format --write src",
    "lint": "mkdir -p eslintreport && yarn lint:check --format html > eslintreport/index.html",
    "lint:check": "yarn g:lint src/**/*.{js,ts,tsx}",
    "lint:fix": "yarn lint:check --fix",
    "typecheck": "yarn g:typecheck",
    "typecheck:watch": "yarn typecheck -w --pretty",
    "start": "concurrently --kill-others --names \"GATSBY,TYPES\" -c \"bgMagenta,bgBlue\" \"yarn develop\" \"yarn typecheck:watch\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "storybook": "start-storybook -p 6006 -s ./static",
    "build-storybook": "build-storybook -s ./static"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.4",
    "@reach/router": "^1.3.4",
    "@react-spring/web": "^9.2.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/user-event": "^13.1.5",
    "@tsw/telemetry": "workspace:packages/utils/telemetry",
    "@tsw/test-util": "workspace:packages/utils/test",
    "@tsw/tracking-util": "workspace:packages/utils/tracking",
    "@tswdts/react-components": "workspace:packages/react-components",
    "@types/camelcase-keys": "^5.1.1",
    "@xstate/inspect": "^0.5.0",
    "@xstate/react": "^1.3.4",
    "axios": "^0.21.1",
    "camelcase-keys": "^6.2.2",
    "d3-array": "^2.12.1",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-create-client-paths": "^3.2.0",
    "gatsby-plugin-google-tagmanager": "^3.9.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-sharp": "^2.14.0",
    "gatsby-plugin-styled-components": "^3.9.0",
    "gatsby-plugin-ts-config": "^1.1.5",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-json": "^3.1.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "jest-styled-components": "^7.0.5",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "pluralize": "^8.0.0",
    "qs": "^6.10.1",
    "react": "^16.12.0",
    "react-countdown": "^2.3.2",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^3.1.1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "styled-components": "^5.2.1",
    "xstate": "^4.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@sinonjs/fake-timers": "^7.1.2",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.1.1",
    "@tsw/eslint-config": "workspace:packages/shared-config/eslint",
    "@tsw/typescript-config": "workspace:packages/shared-config/typescript",
    "@types/axios": "^0.14.0",
    "@types/chart.js": "^2.9.30",
    "@types/d3-array": "^2.9.0",
    "@types/d3-format": "^2.0.0",
    "@types/d3-interpolate-path": "^2.0.0",
    "@types/d3-time-format": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/js-cookie": "^2.2.7",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^5.1.7",
    "@types/testing-library__cypress": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-gatsby": "^0.11.0",
    "babel-preset-react-app": "^10.0.0",
    "concurrently": "^6.0.2",
    "cypress": "^6.3.0",
    "cypress-mochawesome-reporter": "^2.0.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-html-reporter": "^3.3.0",
    "lint-staged": "^10.5.3",
    "mockdate": "^3.0.5",
    "start-server-and-test": "^1.11.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typecheck && yarn test:coverage"
    }
  }
}

{
  "name": "hybrid-frontend",
  "private": true,
  "description": "A Gatsby demo for the Digital Hybrid project",
  "engines": {
    "node": "^12.21.0",
    "yarn": "^2.4.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/{.,**}/*.{js,ts,tsx,md,json}'; yarn lint:fix",
    "format:check": "prettier --check 'src/{.,**}/*.{js,ts,tsx,md,json}'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:coverage": "jest --collectCoverage && yarn jest-coverage-badges output './badges'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "chart.js": "^2.9.4",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-sharp": "^2.14.0",
    "gatsby-plugin-styled-components": "^3.9.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^16.12.0",
    "styled-components": "^5.2.1",
    "gatsby-cli": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/chart.js": "^2.9.30",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/testing-library__cypress": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-gatsby": "^0.11.0",
    "babel-preset-react-app": "^10.0.0",
    "cypress": "^6.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "start-server-and-test": "^1.11.7",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:coverage && git add ./badges/"
    }
  }
}